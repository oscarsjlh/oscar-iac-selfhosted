{
  "repositories": [
    "oscarsjlh/oscar-iac-selfhosted"
  ],
  "branchPrefix": "test-renovate/",
  "dryRun": "full",
  "username": "renovate-release",
  "gitAuthor": "Renovate Bot <bot@renovateapp.com>",
  "onboarding": false,
  "platform": "github",
  "extends": [
    "config:base"
  ],
  "enabledManagers": [
    "docker-compose"
  ],
  "fileMatch": [
    "(?i)^docker/.+/docker-compose/docker-compose\\.ya?ml$"
  ],
  "schedule": [
    "before 3am on monday"
  ],
  "packageRules": [
    {
      "matchDatasources": [
        "docker"
      ],
      "matchPackagePatterns": [
        ".*"
      ],
      "groupName": "all docker image updates"
    }
  ]
}
