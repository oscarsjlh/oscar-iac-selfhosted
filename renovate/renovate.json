{
  "repositories": [
    "oscarsjlh/oscar-iac-selfhosted"
  ],
  "branchPrefix": "test-renovate/",
  "dryRun": "full",
  "username": "renovate-release",
  "gitAuthor": "Renovate Bot <bot@renovateapp.com>",
  "onboarding": false,
  "platform": "github",
  "extends": [
    "config:recommended"
  ],
  "enabledManagers": [
    "docker-compose"
  ],
  "managerFilePatterns": [
    "/(?i)^docker/.+/docker-compose/docker-compose\\.ya?ml$/"
  ],
  "schedule": [
    "before 3am on monday"
  ],
  "packageRules": [
    {
      "matchDatasources": [
        "docker"
      ],
      "groupName": "all docker image updates",
      "matchPackageNames": [
        "/.*/"
      ]
    }
  ]
}
